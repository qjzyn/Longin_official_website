["Exam02.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;AAmBA,eAAA;AACA,MADA,kBACA;AAAA;AACA,WAAA;AACA,aAAA,EADA;AACA;AACA,YAAA,EAFA;AAEA;AACA,YAAA,EAHA,CAGA;AACA;;AAJA,KAAA;AAKA,GAPA;AAQA,WAAA;AACA,OADA,iBACA;AACA;AACA;AACA;AACA,UAAA,OAAA,qBAAA,CAJA,CAKA;;AACA,UAAA,OAAA,aAAA,CANA,CAOA;AACA;;AACA,UAAA,IAAA,KAAA,KAAA,CATA,CAUA;;AACA,UAAA,IAAA,KAAA,IAAA,CAXA,CAYA;;AACA,UAAA,IAAA,KAAA,IAAA,CAbA,CAeA;AACA;;AACA,UAAA,CAAA,KAAA,IAAA,CAAA,CAAA,CAAA,EAAA;AACA;AACA,aAAA,WAAA,CAAA,IAAA,EAAA,UAAA,EAFA,CAGA;;AACA;AACA,OAtBA,CAuBA;;;AACA,UAAA,CAAA,KAAA,IAAA,CAAA,CAAA,CAAA,EAAA;AACA;AACA,aAAA,WAAA,CAAA,IAAA,EAAA,SAAA,EAFA,CAGA;;AACA;AACA,OA7BA,CA8BA;;;AACA,UAAA,CAAA,KAAA,IAAA,CAAA,CAAA,CAAA,EAAA;AACA;AACA,aAAA,WAAA,CAAA,IAAA,EAAA,SAAA,EAFA,CAGA;;AACA;AACA,OApCA,CAqCA;;;AACA,YAAA,MAAA;AACA;AAxCA;AARA,CAAA","sourcesContent":["<<template>\r\n    <div>\r\n        <h1>Exam02.vue</h1>\r\n        <!--调用mint-ui 输入框-->\r\n        <mt-field label=\"用户名\" placeholder=\"请输入用户名\" v-model=\"uname\" :attr=\"{maxlength:8,minlength:3}\"></mt-field>\r\n        <!-- 练习：表单验证 -->\r\n        <!-- 1:为表单添加密码输入框 -->\r\n        <mt-field label=\"密码\" placeholder=\"请输入密码\" type=\"password\" v-model=\"upwd\" :attr=\"{maxlength:8,minlength:3}\"></mt-field>\r\n        <!-- 2:为表单添加年龄输入框 -->\r\n        <mt-field label=\"年龄\" placeholder=\"请输入年龄\" type=\"number\" v-model=\"uage\" :attr=\"{maxlength:2}\"></mt-field>\r\n        <!-- 3:为表单添加注册按钮 -->\r\n        <mt-button size=\"large\" @click=\"reg\">用户注册</mt-button>\r\n        <!-- 4:用户点击注册按钮完成表单验证 -->\r\n        <!-- 用户名:3~8 字母数字 密码验证规则相同 -->\r\n        <!-- 年龄:2 数字 -->\r\n        <!-- 如果用户输入内容有误:明确提示 -->\r\n    </div>\r\n</template>>\r\n<script>\r\nexport default {\r\n    data(){//组件中共享数据\r\n        return {\r\n            uname:\"\",//双向绑定的用户名\r\n            upwd:\"\",//双向绑定的密码\r\n            uage:\"\"//双向绑定的年龄\r\n        }//数据\r\n    },\r\n    methods: {\r\n        reg(){\r\n            //功能：对表单中的每个元素进行验证\r\n            //验证规则\r\n            //1：创建验证所需要的正则表达式 用户名/密码通用\r\n            var ureg=/^[a-z0-9A-Z]{3,8}$/i;\r\n            //2：创建验证所需要的正则表达式 年龄\r\n            var areg=/^[0-9]{2}$/i;\r\n            //验证谁\r\n            //3：获取用户名\r\n            var u=this.uname;\r\n            //4：获取密码\r\n            var p=this.upwd;\r\n            //5：获取年龄\r\n            var a=this.uage;\r\n\r\n            //验证\r\n            //6；通过正则表达式验证用户名\r\n            if(!ureg.test(u)){\r\n            //7：出错显示交互的提示消息\r\n                this.$messagebox(\"消息\",\"用户名格式不正确\");\r\n            //8:程序停止\r\n            return;\r\n            }\r\n            //9:通过正则表达式验证密码\r\n            if(!ureg.test(p)){\r\n            //10：出错显示交互提示消息\r\n                this.$messagebox(\"消息\",\"密码格式不正确\");\r\n            //11：程序停止\r\n            return;\r\n            }\r\n            //12：通过正则表达式验证年龄\r\n            if(!areg.test(a)){\r\n            //13：出错显示交互提示消息\r\n                this.$messagebox(\"消息\",\"年龄格式不正确\");\r\n            //14：程序停止\r\n            return;\r\n            }\r\n            //15：输出验证成功\r\n            alert(\"验证成功\")\r\n        }\r\n    }\r\n}\r\n</script>\r\n<style></style>\r\n"],"sourceRoot":"src\\components\\exam"}]}