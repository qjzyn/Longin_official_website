n        openPrompt(){\r\n            this.$messagebox.prompt(\"请输入您的充值金额\")//输入框\r\n            .then((value,action)=>{//回调函数\r\n                console.log(value);//输入数值\r\n                console.log(action);\r\n            }).catch(err=>{//取消函数\r\n                console.log(err);//取消\r\n            });\r\n        },\r\n        openConfirm(){\r\n            //功能：显示确认的消息框\r\n            this.$messagebox.confirm(\"是否确定充值648礼包？\")//消息内容\r\n            .then(action=>{\r\n                //用户选择确认回调函数\r\n                console.log(\"充值成功\")\r\n            })\r\n            .catch(err=>{\r\n                //用户选择取消回调函数\r\n                console.log(\"充值失败\")\r\n            })\r\n        },\r\n        openAlert(){\r\n            //交互提示框\r\n            //1.标准的方式显示提示框\r\n            //this.$messagebox({\r\n                //title:\"消息\",\r\n                //message:\"执行操作成功\"\r\n            //});\r\n            this.$messagebox(\"恭喜您充值648礼包\",\"充值成功！\")\r\n        },\r\n        openToast(){\r\n            //功能：此函数完成显示mint-ui短信息\r\n            //提示框\r\n            this.$toast({\r\n                message:\"操作成功\", //提示文字\r\n                postition:\"top\", //提示文字位置顶端\r\n                duration:700, //持续时间\r\n                iconClass:\"iconfont icon-home\"//使用字体的名称      图标名称(首页)\r\n            });\r\n            //console.log(this)\r\n        }\r\n    },\r\n}\r\n</script>\r\n<style>\r\n    /*当前组件专有的样式内容*/\r\n</style>"],"sourceRoot":"src\\components\\exam"}]}